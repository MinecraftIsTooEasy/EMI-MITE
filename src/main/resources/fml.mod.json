{
  "schemaVersion": 1,
  "id": "emi",
  "version": "${version}",
  "name": "EMI",
  "description": "A featureful and accessible item and recipe viewer",
  "authors": [
    "Emi",
    "Katherine",
    "Bagel",
    "Xy_Lose"
  ],
  "contributors": [
    "limingzxc",
    "Huix"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "moddedmite.emi.platform.fish.EmiMainFish"
    ],
    "client": [
      "dev.emi.emi.platform.EmiClient"
    ],
    "server": [],
    "emi": [
      "dev.emi.emi.VanillaPlugin",
      "moddedmite.emi.MITEPlugin"
    ],
    "modmenu": [
      "dev.emi.emi.compat.EmiModMenu"
    ]
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/ItemStack": [
        "moddedmite/emi/api/EMIItemStack"
      ]
    }
  },
  "mixins": [
    "emi.mixins.json"
  ],
  "accessWidener": "emi.accesswidener",
  "depends": {
    "minecraft": "1.6.4-mite",
    "fishmodloader": ">=3.4.0"
  },
  "suggests": {
    "hwite": ">=1.4.7",
    "pinin": ">=1.6.0"
  }
}